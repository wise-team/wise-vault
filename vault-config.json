{
    "backend": {
        "file": {
            "path": "/vault/backend_file"
        }
    },
     "default_lease_ttl": "168h",
    "max_lease_ttl": "720h",
    "listener": [
        {
            "tcp": {
                "address": "0.0.0.0:8200",
                "tls_disable": 0,
                "tls_cert_file": "/etc/certs/vault-localhost.crt",
                "tls_key_file": "/etc/certs/vault-localhost.key"
            }
        }
    ]
}